                                            <div class="description mb-50">
                                                <%= product.About_author%>

                                                    <hr class="wp-block-separator is-style-dots">

                                            </div>
                                            <h3 class="section-title style-1 mb-30">Additional info</h3>
                                            <table class="font-md mb-30">
                                                <tbody>
                                                    <tr class="stand-up">
                                                        <th>Publisher</th>
                                                        <td>
                                                            <p>
                                                                <%= product.publisher%>
                                                            </p>
                                                        </td>
                                                    </tr>
                                                    <tr class="folded-wo-wheels">
                                                        <th>Language</th>
                                                        <td>
                                                            <p>
                                                                <%= product.language%>
                                                            </p>
                                                        </td>
                                                    </tr>
                                                    <tr class="folded-w-wheels">
                                                        <th>Country of Origin</th>
                                                        <td>
                                                            <p>
                                                                <%= product.country%>
                                                            </p>
                                                        </td>
                                                    </tr>
                                                    <tr class="door-pass-through">
                                                        <th>Item Weight </th>
                                                        <td>
                                                            <p>
                                                                <%= product.item_weight%> g
                                                            </p>
                                                        </td>
                                                    </tr>
                                                    <tr class="frame">
                                                        <th>Paperback</th>
                                                        <td>
                                                            <p>
                                                                <%= product.paperback%> pages
                                                            </p>
                                                        </td>
                                                    </tr>

                                                </tbody>
                                            </table>
                                           
                                        
                                        </div>
                                    </div>

                                    <div class="row mt-60">
                                        <div class="col-12">
                                            <h3 class="section-title style-1 mb-30">Related products</h3>
                                        </div>
                                        <% if(products.length> 0) { %>
                                            <% for(let i=0; i < Math.min(8, products.length); i++) { %>
                                                <% let productt=products[i]; %>
                                                    <div class="col-lg-3 col-md-4 col-12 col-sm-6">
