                                                                    <div>
                                                                        <img src="/productimages/<%= product.image[i] %>"
                                                                            alt="<%= product.image[i] %>"
                                                                            class="img-thumbnail" style="width: 100%;
                                                height: 100%; object-fit:cover;">

                                                                    </div>
                                                                    <% } %>
                                                                        <% } else { %>
                                                                            <p>No images available</p>
                                                                            <% } %>

                                            </div>
                                        </div>
                                        <!-- End Gallery -->
                                    </div>
                                    <div class="col-md-6 col-sm-12 col-xs-12">
